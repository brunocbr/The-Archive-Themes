    "styles": {
        "base": {
            "color": "#1a1a1a"
        },

        "h1": {
            "font": { "style": "bold" },
            "color": "#911e07",
            "syntax": { "color" : "#1a1a1a" }
        },
        "h2": {
            "font": {  "style": "bold" },
