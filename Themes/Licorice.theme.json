            "syntax": { "color" : "#1a1a1a" }
        },
        "h3": {
            "font": {  "style": "bold" },
            "color": "#911e07",
            "syntax": { "color" : "#1a1a1a" }
        },
        "h4": {
            "font": {  "style": "bold" },
            "color": "#911e07",
            "syntax": { "color" : "#1a1a1a" }
        },
        "h5": {
            "font": {  "style": "bold" },
            "color": "#911e07",
            "syntax": { "color" : "#1a1a1a" }
